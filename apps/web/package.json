{
  "name": "@customerly/web",
  "description": "A production ready Next.js/Supabase starter. ",
  "version": "0.1.0",
  "private": true,
  "author": "Michael Troya <hi@michaeltroya.ca> (@michaeltroya)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "supabase",
    "tailwindcss",
    "shadcn",
    "typescript"
  ],
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .turbo node_modules .next",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "type-check": "tsc --pretty --noEmit",
    "prepare": "cd ../.. && husky install apps/web/.husky",
    "test": "npm run test:e2e",
    "test:unit": "echo \"Unit tests temporarily disabled\"",
    "test:e2e": "dotenv -e test.env -- playwright test",
    "test:e2e:ui": "dotenv -e test.env -- playwright test --ui",
    "test:e2e:debug": "dotenv -e test.env -- playwright test --debug",
    "test:ci": "jest --ci",
    "storage:policies": "npm run storage:policies:prod && npm run storage:policies:test",
    "storage:policies:prod": "dotenv -e .env -- ts-node scripts/apply-storage-policies.ts",
    "storage:policies:test": "dotenv -e test.env -- ts-node scripts/apply-storage-policies.ts"
  },
  "dependencies": {
    "@next/bundle-analyzer": "14.2.23",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-dialog": "1.1.5",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-popover": "1.1.5",
    "@radix-ui/react-select": "2.1.5",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-toast": "1.2.5",
    "@supabase/auth-helpers-nextjs": "0.9.0",
    "@supabase/ssr": "0.1.0",
    "@tanstack/react-query": "5.17.15",
    "@tanstack/react-query-devtools": "5.17.15",
    "@vercel/analytics": "1.1.1",
    "axios": "1.7.9",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "geist": "1.2.0",
    "lucide-react": "0.312.0",
    "next": "14.1.0",
    "next-themes": "0.2.1",
    "nextjs-toploader": "1.6.4",
    "postcss-nested": "7.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "2.2.0",
    "uuid": "11.0.5"
  },
  "devDependencies": {
    "@customerly/db": "0.1.0",
    "@playwright/test": "1.50.0",
    "@shadcn/ui": "0.0.4",
    "@supabase/supabase-js": "2.48.1",
    "@swc/core": "1.3.102",
    "@swc/jest": "0.2.29",
    "@testing-library/dom": "9.3.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "20.17.16",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "10.0.0",
    "autoprefixer": "10.4.14",
    "dotenv-cli": "8.0.0",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.3",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "15.2.0",
    "msw": "2.7.0",
    "postcss": "8.4.23",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "start-server-and-test": "2.0.10",
    "tailwindcss": "3.3.0",
    "tailwindcss-animate": "1.0.7",
    "tsx": "4.19.2",
    "typescript": "^5.3.3"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=18.0.0",
    "npm": "10.9.2"
  }
}
