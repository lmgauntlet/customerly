    "lint-staged": "15.2.0",
    "msw": "2.1.1",
    "postcss": "8.4.33",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.5.14",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=18.0.0",
    "npm": "10.9.2"
  }
