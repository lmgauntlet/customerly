{
    "name": "customerly",
    "private": true,
    "scripts": {
        "build": "turbo build",
        "dev": "turbo dev",
        "lint": "turbo lint",
        "test": "turbo test",
        "clean": "turbo clean && rm -rf node_modules",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "postinstall": "npm run build"
    },
    "workspaces": [
        "apps/*"
    ],
    "devDependencies": {
        "prettier": "3.2.5",
        "turbo": "2.3.3",
        "typescript": "5.3.3"
    },
    "packageManager": "npm@10.9.2",
    "engines": {
        "node": ">=18.0.0",
        "npm": "10.9.2"
    }
}