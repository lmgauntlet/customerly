{
    "name": "@customerly/db",
    "version": "0.1.0",
    "private": true,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rm -rf .turbo node_modules dist",
        "build": "tsc",
        "dev": "tsc -w",
        "lint": "eslint .",
        "format": "prettier --write .",
        "db:migrate": "prisma migrate deploy",
        "db:createmigration": "prisma migrate dev --create-only --name",
        "db:createmigrationpost": "ts-node prisma/migration-generator.ts",
        "db:seed": "node --no-warnings=DEP0040 -r ts-node/register scripts/seed.ts",
        "db:reset": "node --no-warnings=DEP0040 -r ts-node/register scripts/reset-db.ts",
        "test": "echo \"Unit tests temporarily disabled\""
    },
    "dependencies": {
        "@prisma/client": "5.10.0",
        "@supabase/supabase-js": "2.39.0",
        "commander": "11.1.0",
        "dotenv": "16.3.1",
        "pg": "8.11.3"
    },
    "devDependencies": {
        "@types/jest": "29.5.11",
        "@types/node": "20.11.5",
        "@types/pg": "8.11.0",
        "@typescript-eslint/eslint-plugin": "6.19.0",
        "@typescript-eslint/parser": "6.19.0",
        "eslint": "8.56.0",
        "jest": "29.7.0",
        "prisma": "5.10.0",
        "ts-jest": "29.1.1",
        "ts-node": "10.9.2",
        "typescript": "5.3.3",
        "prettier": "3.4.2"
    },
    "packageManager": "npm@10.9.2",
    "engines": {
        "node": ">=18.0.0",
        "npm": "10.9.2"
    }
}